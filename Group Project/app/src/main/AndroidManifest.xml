<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mustang_clone">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cobramustangview"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cobramustangview"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mustang_Clone">

        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Category" android:exported="false" />
        <activity android:name=".Category_Add" android:exported="false" />
        <activity android:name=".Category_Update" android:exported="false" />
        <activity android:name=".CarItem" android:exported="false" />
        <activity android:name=".Car_Add" android:exported="false" />
        <activity android:name=".Car_Update" android:exported="false" />
        <activity android:name=".CarView" android:exported="false" />

    </application>
</manifest>
